{%- block instantiate_calo_calo_calo_correlation_orm_condition %}
  {%- set o1 = condition.objects[0] %}
  {%- set o2 = condition.objects[1] %}
  {%- if condition.nr_objects == 3 %}
  {%- set o3 = condition.objects[2] %}
{{ condition.vhdl_signal }}_i: entity work.calo_calo_calo_correlation_orm_condition
    generic map(
        true,
        {{ condition.deltaEtaOrm.enabled }}, {{ condition.deltaPhiOrm.enabled }}, {{ condition.deltaROrm.enabled }},
        {{ condition.deltaEta.enabled }}, {{ condition.deltaPhi.enabled }}, {{ condition.deltaR.enabled }}, {{ condition.mass.enabled }}, {{ condition.mass.type }}, {{ condition.twoBodyPt.enabled }},
        NR_{{ o1.type|upper }}_OBJECTS,        
        {{ o1.sliceLow }}, {{ o1.sliceHigh }}, {{ o1.operator }}, {{ o1.type|upper }}_TYPE,
        X"{{ o1.threshold|X04 }}",
        {{ o1.etaNrCuts }},
        X"{{ o1.etaW1UpperLimit|X04 }}", X"{{ o1.etaW1LowerLimit|X04 }}",
        X"{{ o1.etaW2UpperLimit|X04 }}", X"{{ o1.etaW2LowerLimit|X04 }}",
        X"{{ o1.etaW3UpperLimit|X04 }}", X"{{ o1.etaW3LowerLimit|X04 }}",
        X"{{ o1.etaW4UpperLimit|X04 }}", X"{{ o1.etaW4LowerLimit|X04 }}",
        X"{{ o1.etaW5UpperLimit|X04 }}", X"{{ o1.etaW5LowerLimit|X04 }}",
        {{ o1.phiFullRange }}, X"{{ o1.phiW1UpperLimit|X04 }}", X"{{ o1.phiW1LowerLimit|X04 }}",
        {{ o1.phiW2Ignore }}, X"{{ o1.phiW2UpperLimit|X04 }}", X"{{ o1.phiW2LowerLimit|X04 }}",
        X"{{ o1.isolationLUT|X01 }}",
        NR_{{ o3.type|upper }}_OBJECTS,        
        {{ o3.sliceLow }}, {{ o3.sliceHigh }}, {{ o3.operator }}, {{ o3.type|upper }}_TYPE,
        X"{{ o3.threshold|X04 }}",
        {{ o3.etaNrCuts }}, 
        X"{{ o3.etaW1UpperLimit|X04 }}", X"{{ o3.etaW1LowerLimit|X04 }}",
        X"{{ o3.etaW2UpperLimit|X04 }}", X"{{ o3.etaW2LowerLimit|X04 }}",
        X"{{ o3.etaW3UpperLimit|X04 }}", X"{{ o3.etaW3LowerLimit|X04 }}",
        X"{{ o3.etaW4UpperLimit|X04 }}", X"{{ o3.etaW4LowerLimit|X04 }}",
        X"{{ o3.etaW5UpperLimit|X04 }}", X"{{ o3.etaW5LowerLimit|X04 }}",
        {{ o3.phiFullRange }}, X"{{ o3.phiW1UpperLimit|X04 }}", X"{{ o3.phiW1LowerLimit|X04 }}",
        {{ o3.phiW2Ignore }}, X"{{ o3.phiW2UpperLimit|X04 }}", X"{{ o3.phiW2LowerLimit|X04 }}",
        X"{{ o3.isolationLUT|X01 }}",
        X"{{ condition.deltaEtaOrm.upper|X08 }}", X"{{ condition.deltaEtaOrm.lower|X08 }}",
        X"{{ condition.deltaPhiOrm.upper|X08 }}", X"{{ condition.deltaPhiOrm.lower|X08 }}",
        X"{{ condition.deltaROrm.upper|X16 }}", X"{{ condition.deltaROrm.lower|X16 }}",
        X"{{ condition.deltaEta.upper|X08 }}", X"{{ condition.deltaEta.lower|X08 }}",
        X"{{ condition.deltaPhi.upper|X08 }}", X"{{ condition.deltaPhi.lower|X08 }}",
        X"{{ condition.deltaR.upper|X16 }}", X"{{ condition.deltaR.lower|X16 }}",
        X"{{ condition.mass.upper|X16 }}", X"{{ condition.mass.lower|X16 }}",
        X"{{ condition.twoBodyPt.threshold|X16 }}", 
    {%- if condition.mass.enabled == 'true' %}        
        {{ o1.type|upper }}_{{ o2.type|upper }}_MASS_VECTOR_WIDTH,
    {%- endif %}
    {%- if condition.twoBodyPt.enabled == 'true' %}        
        tbpt_width => {{ o1.type|upper }}_{{ o2.type|upper }}_TBPT_VECTOR_WIDTH,
    {%- endif %}
        nr_obj_calo2 => NR_{{ o2.type|upper }}_OBJECTS,        
        calo2_object_low => {{ o2.sliceLow }}, calo2_object_high => {{ o2.sliceHigh }}, et_ge_mode_calo2 => {{ o2.operator }}, obj_type_calo2 => {{ o2.type|upper }}_TYPE,
        et_threshold_calo2 => X"{{ o2.threshold|X04 }}",
        nr_eta_windows_calo2 => {{ o2.etaNrCuts }}, 
        eta_w1_upper_limit_calo2 => X"{{ o2.etaW1UpperLimit|X04 }}", eta_w1_lower_limit_calo2 => X"{{ o2.etaW1LowerLimit|X04 }}",
        eta_w2_upper_limit_calo2 => X"{{ o2.etaW2UpperLimit|X04 }}", eta_w2_lower_limit_calo2 => X"{{ o2.etaW2LowerLimit|X04 }}",
        eta_w3_upper_limit_calo2 => X"{{ o2.etaW3UpperLimit|X04 }}", eta_w3_lower_limit_calo2 => X"{{ o2.etaW3LowerLimit|X04 }}",
        eta_w4_upper_limit_calo2 => X"{{ o2.etaW4UpperLimit|X04 }}", eta_w4_lower_limit_calo2 => X"{{ o2.etaW4LowerLimit|X04 }}",
        eta_w5_upper_limit_calo2 => X"{{ o2.etaW5UpperLimit|X04 }}", eta_w5_lower_limit_calo2 => X"{{ o2.etaW5LowerLimit|X04 }}",
        phi_full_range_calo2 => {{ o2.phiFullRange }}, 
        phi_w1_upper_limit_calo2 => X"{{ o2.phiW1UpperLimit|X04 }}", phi_w1_lower_limit_calo2 => X"{{ o2.phiW1LowerLimit|X04 }}",
        phi_w2_ignore_calo2 => {{ o2.phiW2Ignore }}, 
        phi_w2_upper_limit_calo2 => X"{{ o2.phiW2UpperLimit|X04 }}", phi_w2_lower_limit_calo2 => X"{{ o2.phiW2LowerLimit|X04 }}",
        iso_lut_calo2 => X"{{ o2.isolationLUT|X01 }}"
    )
    port map(lhc_clk, {{ o1.type|lower }}_bx_{{ o1.bx }}, {{ o2.type|lower }}_bx_{{ o2.bx }}, {{ o3.type|lower }}_bx_{{ o3.bx }},
    {%- if condition.deltaEtaOrm.enabled == 'true' %}
        deta_orm => {{ o1.type|lower }}_{{ o3.type|lower }}_bx_{{ o1.bx }}_bx_{{ o3.bx }}_deta_vector,
    {%- endif %}
    {%- if condition.deltaPhiOrm.enabled == 'true' %}    
        dphi_orm => {{ o1.type|lower }}_{{ o3.type|lower }}_bx_{{ o1.bx }}_bx_{{ o3.bx }}_dphi_vector,
    {%- endif %}
    {%- if condition.deltaROrm.enabled == 'true' %}    
        dr_orm => {{ o1.type|lower }}_{{ o3.type|lower }}_bx_{{ o1.bx }}_bx_{{ o3.bx }}_delta_r_vector,
    {%- endif %}
    {%- if condition.deltaEta.enabled == 'true' %}
        deta_12 => {{ o1.type|lower }}_{{ o2.type|lower }}_bx_{{ o1.bx }}_bx_{{ o2.bx }}_deta_vector,
    {%- endif %}
    {%- if condition.deltaPhi.enabled == 'true' %}    
        dphi_12 => {{ o1.type|lower }}_{{ o2.type|lower }}_bx_{{ o1.bx }}_bx_{{ o2.bx }}_dphi_vector,
    {%- endif %}
    {%- if condition.deltaR.enabled == 'true' %}    
        dr_12 => {{ o1.type|lower }}_{{ o2.type|lower }}_bx_{{ o1.bx }}_bx_{{ o2.bx }}_delta_r_vector,
    {%- endif %}
    {%- if condition.mass.enabled == 'true' and condition.mass.type == 0 %}    
        mass_inv_12 => {{ o1.type|lower }}_{{ o2.type|lower }}_bx_{{ o1.bx }}_bx_{{ o2.bx }}_mass_inv_vector,
    {%- endif %}
    {%- if condition.twoBodyPt.enabled == 'true' %}    
        tbpt_12 => {{ o1.type|lower }}_{{ o2.type|lower }}_bx_{{ o1.bx }}_bx_{{ o2.bx }}_tbpt_vector,
    {%- endif %}
        condition_o => {{ condition.vhdl_signal }});
  {%- elif  condition.nr_objects == 2 %}
{{ condition.vhdl_signal }}_i: entity work.calo_calo_calo_correlation_orm_condition
    generic map(
        false,
        {{ condition.deltaEtaOrm.enabled }}, {{ condition.deltaPhiOrm.enabled }}, {{ condition.deltaROrm.enabled }},
        {{ condition.deltaEta.enabled }}, {{ condition.deltaPhi.enabled }}, {{ condition.deltaR.enabled }}, {{ condition.mass.enabled }}, {{ condition.mass.type }}, {{ condition.twoBodyPt.enabled }},
        NR_{{ o1.type|upper }}_OBJECTS,        
        {{ o1.sliceLow }}, {{ o1.sliceHigh }}, {{ o1.operator }}, {{ o1.type|upper }}_TYPE,
        X"{{ o1.threshold|X04 }}",
        {{ o1.etaNrCuts }},
        X"{{ o1.etaW1UpperLimit|X04 }}", X"{{ o1.etaW1LowerLimit|X04 }}",
        X"{{ o1.etaW2UpperLimit|X04 }}", X"{{ o1.etaW2LowerLimit|X04 }}",
        X"{{ o1.etaW3UpperLimit|X04 }}", X"{{ o1.etaW3LowerLimit|X04 }}",
        X"{{ o1.etaW4UpperLimit|X04 }}", X"{{ o1.etaW4LowerLimit|X04 }}",
        X"{{ o1.etaW5UpperLimit|X04 }}", X"{{ o1.etaW5LowerLimit|X04 }}",
        {{ o1.phiFullRange }}, X"{{ o1.phiW1UpperLimit|X04 }}", X"{{ o1.phiW1LowerLimit|X04 }}",
        {{ o1.phiW2Ignore }}, X"{{ o1.phiW2UpperLimit|X04 }}", X"{{ o1.phiW2LowerLimit|X04 }}",
        X"{{ o1.isolationLUT|X01 }}",
        NR_{{ o2.type|upper }}_OBJECTS,        
        {{ o2.sliceLow }}, {{ o2.sliceHigh }}, {{ o2.operator }}, {{ o2.type|upper }}_TYPE,
        X"{{ o2.threshold|X04 }}",
        {{ o2.etaNrCuts }}, 
        X"{{ o2.etaW1UpperLimit|X04 }}", X"{{ o2.etaW1LowerLimit|X04 }}",
        X"{{ o2.etaW2UpperLimit|X04 }}", X"{{ o2.etaW2LowerLimit|X04 }}",
        X"{{ o2.etaW3UpperLimit|X04 }}", X"{{ o2.etaW3LowerLimit|X04 }}",
        X"{{ o2.etaW4UpperLimit|X04 }}", X"{{ o2.etaW4LowerLimit|X04 }}",
        X"{{ o2.etaW5UpperLimit|X04 }}", X"{{ o2.etaW5LowerLimit|X04 }}",
        {{ o2.phiFullRange }}, X"{{ o2.phiW1UpperLimit|X04 }}", X"{{ o2.phiW1LowerLimit|X04 }}",
        {{ o2.phiW2Ignore }}, X"{{ o2.phiW2UpperLimit|X04 }}", X"{{ o2.phiW2LowerLimit|X04 }}",
        X"{{ o2.isolationLUT|X01 }}",
        X"{{ condition.deltaEtaOrm.upper|X08 }}", X"{{ condition.deltaEtaOrm.lower|X08 }}",
        X"{{ condition.deltaPhiOrm.upper|X08 }}", X"{{ condition.deltaPhiOrm.lower|X08 }}",
        X"{{ condition.deltaROrm.upper|X16 }}", X"{{ condition.deltaROrm.lower|X16 }}",
        X"{{ condition.deltaEta.upper|X08 }}", X"{{ condition.deltaEta.lower|X08 }}",
        X"{{ condition.deltaPhi.upper|X08 }}", X"{{ condition.deltaPhi.lower|X08 }}",
        X"{{ condition.deltaR.upper|X16 }}", X"{{ condition.deltaR.lower|X16 }}",
        X"{{ condition.mass.upper|X16 }}", X"{{ condition.mass.lower|X16 }}",
        X"{{ condition.twoBodyPt.threshold|X16 }}", 
    {%- if condition.mass.enabled == 'true' and condition.twoBodyPt.enabled == 'true' %}        
        {{ o1.type|upper }}_{{ o2.type|upper }}_MASS_VECTOR_WIDTH,
        {{ o1.type|upper }}_{{ o2.type|upper }}_TBPT_VECTOR_WIDTH
    {%- elif condition.mass.enabled == 'true' and condition.twoBodyPt.enabled == 'false' %}
        {{ o1.type|upper }}_{{ o2.type|upper }}_MASS_VECTOR_WIDTH
    {%- elif condition.mass.enabled == 'false' and condition.twoBodyPt.enabled == 'true' %}
        {{ o1.type|upper }}_{{ o2.type|upper }}_TBPT_VECTOR_WIDTH
    {%- endif %}
    )
    port map(lhc_clk, {{ o1.type|lower }}_bx_{{ o1.bx }}, calo3 => {{ o2.type|lower }}_bx_{{ o2.bx }},
    {%- if condition.deltaEtaOrm.enabled == 'true' %}
        deta_orm => {{ o1.type|lower }}_{{ o2.type|lower }}_bx_{{ o1.bx }}_bx_{{ o2.bx }}_deta_vector,
    {%- endif %}
    {%- if condition.deltaPhiOrm.enabled == 'true' %}    
        dphi_orm => {{ o1.type|lower }}_{{ o2.type|lower }}_bx_{{ o1.bx }}_bx_{{ o2.bx }}_dphi_vector,
    {%- endif %}
    {%- if condition.deltaROrm.enabled == 'true' %}    
        dr_orm => {{ o1.type|lower }}_{{ o2.type|lower }}_bx_{{ o1.bx }}_bx_{{ o2.bx }}_delta_r_vector,
    {%- endif %}
    {%- if condition.deltaEta.enabled == 'true' %}
        deta_13 => {{ o1.type|lower }}_{{ o2.type|lower }}_bx_{{ o1.bx }}_bx_{{ o2.bx }}_deta_vector,
    {%- endif %}
    {%- if condition.deltaPhi.enabled == 'true' %}    
        dphi_13 => {{ o1.type|lower }}_{{ o2.type|lower }}_bx_{{ o1.bx }}_bx_{{ o2.bx }}_dphi_vector,
    {%- endif %}
    {%- if condition.deltaR.enabled == 'true' %}    
        dr_13 => {{ o1.type|lower }}_{{ o2.type|lower }}_bx_{{ o1.bx }}_bx_{{ o2.bx }}_delta_r_vector,
    {%- endif %}
    {%- if condition.mass.enabled == 'true' and condition.mass.type == 0 %}    
        mass_inv_13 => {{ o1.type|lower }}_{{ o2.type|lower }}_bx_{{ o1.bx }}_bx_{{ o2.bx }}_mass_inv_vector,
    {%- endif %}
    {%- if condition.twoBodyPt.enabled == 'true' %}    
        tbpt_13 => {{ o1.type|lower }}_{{ o2.type|lower }}_bx_{{ o1.bx }}_bx_{{ o2.bx }}_tbpt_vector,
    {%- endif %}
        condition_o => {{ condition.vhdl_signal }});
  {%- endif %}
{%- endblock instantiate_calo_calo_calo_correlation_orm_condition %}
